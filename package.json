{
  "name": "tanstack-start",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun runner.ts",
    "dev:vite": "vite dev",
    "dev:partykit": "bunx partykit dev",
    "build": "drizzle-kit migrate && vite build",
    "deploy:partykit": "bunx partykit deploy --name tanstack-start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "tsc": "tsc",
    "db:auth:migrate": "bun x @better-auth/cli migrate --config src/lib/auth.ts",
    "db:auth:generate": "bun x @better-auth/cli generate --config src/lib/auth.ts",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:status": "drizzle-kit status",
    "unit:test": "bun test",
    "e2e:test": "bunx playwright test",
    "prepare": "husky"
  },
  "devDependencies": {
